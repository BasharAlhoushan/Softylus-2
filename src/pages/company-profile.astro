  /* Innovation Lab */
  .innovation-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 2rem;
  }

  .innovation-card {
    padding: 2rem;
    border: 1px solid rgba(255, 255, 255, 0.1);
    background: rgba(255, 255, 255, 0.03);
    position: relative;
    overflow: hidden;
  }

  .innovation-stats {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 1rem;
    margin-top: 2rem;
    padding-top: 2rem;
    border-top: 1px solid rgba(255, 255, 255, 0.1);
  }

  .stat {
    text-align: center;
  }

  .stat-value {
    font-size: 1.5rem;
    font-weight: bold;
    color: white;
    margin-bottom: 0.25rem;
  }

  .stat-label {
    font-size: 0.875rem;
    color: rgba(255, 255, 255, 0.5);
  }

  /* Success Stories */
  .success-stories-grid {
    display: grid;
    gap: 2rem;
  }

  .success-story {
    display: grid;
    grid-template-columns: 1fr 2fr;
    gap: 2rem;
    background: white;
    border: 1px solid rgba(0, 0, 0, 0.1);
    overflow: hidden;
  }

  .story-image {
    aspect-ratio: 1;
    overflow: hidden;
  }

  .story-content {
    padding: 2rem;
  }

  .story-tag {
    display: inline-block;
    padding: 0.25rem 0.75rem;
    background: black;
    color: white;
    font-size: 0.75rem;
    font-weight: 500;
    margin-bottom: 1rem;
  }

  .story-metrics {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 2rem;
    margin-top: 2rem;
    padding-top: 2rem;
    border-top: 1px solid rgba(0, 0, 0, 0.1);
  }

  /* Sustainability */
  .sustainability-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 2rem;
  }

  .sustainability-card {
    padding: 2rem;
    border: 1px solid rgba(255, 255, 255, 0.1);
    background: rgba(255, 255, 255, 0.03);
    position: relative;
    overflow: hidden;
  }

  .sustainability-metric {
    margin-top: 2rem;
    padding-top: 2rem;
    border-top: 1px solid rgba(255, 255, 255, 0.1);
    text-align: center;
  }

  .sustainability-metric .metric-value {
    font-size: 2rem;
    font-weight: bold;
    color: white;
    margin-bottom: 0.25rem;
  }

  .sustainability-metric .metric-label {
    font-size: 0.875rem;
    color: rgba(255, 255, 255, 0.5);
  }

  /* Additional Responsive Styles */
  @media (max-width: 768px) {
    .innovation-grid,
    .sustainability-grid {
      grid-template-columns: 1fr;
    }

    .success-story {
      grid-template-columns: 1fr;
    }

    .story-image {
      aspect-ratio: 16/9;
    }
  }

